{{!-- {{this.count}} (total) --}}
<h4>{{this.title}}</h4>
<ul class="list-unstyled">{{#each rows}}
	<li class="triage-query-item">
		{{!-- {{#equal this.llgod_type "aoi_nom" }}
		<span data-id="{{this.llgod_type}}" data-bbox="{{this.boundingbox}}" data-target="{{this.display_name}}" class="triage-trigger">{{nominatum_chop this.display_name 0}}  ({{this.type}}) {{nominatum_chop this.display_name 1}} {{nominatum_chop this.display_name 2}}</span>
		{{/equal}}
		{{#equal this.llgod_type "aoi_custom" }} --}}
		<span data-id="{{this.llgod_type}}" data-bbox="{{this.boundingbox}}" data-target="{{this.display_name}}"  class="triage-trigger">{{#equal this.llgod_type "aoi_nom" }}{{nominatum_chop this.display_name 0}}  ({{this.type}}) {{nominatum_chop this.display_name 1}} {{nominatum_chop this.display_name 2}}{{/equal}}{{#equal this.llgod_type "aoi_custom" }}{{this.display_name}}  ({{this.type}}){{/equal}}</span>
		{{!-- {{/equal}} --}}
		<span class="{{nominatum_iconify this.osm_type}}"></span>
	</li>
	{{/each}}
</ul>